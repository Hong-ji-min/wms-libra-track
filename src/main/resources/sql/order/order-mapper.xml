<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sh.model.dao.OrderDAO">
    <insert id="insertOrder" useGeneratedKeys="true" keyProperty="orderId">
        insert into
        order
        values (
        null, #{quantity}, #{bookId}, #{inWarehousingId}, #{null}
        )
    </insert>
    <select id="findOrder" resultType="orderDto">
        select
            quantity, ISBN, title, author
        from
            order
        left join
            book on order.book_id = book.book_id
    </select>


    <resultMap id="orderDtoMap" type="menuDto">
        <result column="menu_code" property="menuCode"/>
        <result column="menu_name" property="menuName"/>
        <result column="menu_price" property="menuPrice"/>
        <result column="category_code" property="categoryCode"/>
        <result column="orderable_status" property="orderableStatus"/>
    </resultMap>
</mapper>