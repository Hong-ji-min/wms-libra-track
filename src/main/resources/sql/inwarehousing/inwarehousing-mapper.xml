<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sh.model.dao.InWarehousingDAO">
    <insert id="insertInWarehousing" useGeneratedKeys="true" keyProperty="inWarehousingId">
        insert into
        in_warehousing
        values (
        null, #{date}, #{status}, #{publisher_manager_id}, #{null}
        )
    </insert>
    <select id="findInWarehousingByStatus" resultType="InWarehousing">
        select
            *
        from
            in_warehousing
        where
            status = #{status}
    </select>
</mapper>